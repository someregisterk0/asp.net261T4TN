@model List<WebApp.Models.Member>

<table class="table table-bordered">
    <thead class="table-primary">
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Gender</th>
            <th>Edit</th>
        </tr>
    </thead>
    <tbody>
        @foreach (WebApp.Models.Member item in Model)
        {
            <tr>
                <td>@item.Username</td>
                <td>@item.Email</td>
                <td>@item.Gender</td>
                <td><a href="/member/roles/@item.Id">Roles</a></td>
            </tr>
        }
    </tbody>
</table>